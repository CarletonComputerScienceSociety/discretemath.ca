{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "env-cmd rollup -c",
    "dev": "env-cmd rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "test": "jest",
    "check": "prettier --check '{public,src}/**/*.{css,html,js,ts,svelte}'",
    "format": "prettier --write '{public,src}/**/*.{css,html,js,ts,svelte}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-svelte-csf": "^1.0.0",
    "@storybook/svelte": "^6.2.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/jest": "^26.0.14",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.2.2",
    "jest": "^26.5.2",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.49.0",
    "svelte-check": "^1.0.0",
    "svelte-jester": "^1.1.5",
    "svelte-loader": "^3.1.1",
    "svelte-preprocess": "^4.7.2",
    "ts-jest": "^26.4.1",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@lottiefiles/svelte-lottie-player": "^0.2.0",
    "apollo": "^2.33.4",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "env-cmd": "^10.1.0",
    "eslint": "^7.25.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "graphql": "14.0.2",
    "graphql-tag": "^2.12.4",
    "prettier": "^2.3.0",
    "prettier-plugin-svelte": "^2.3.0",
    "sass": "^1.32.12",
    "sirv-cli": "^1.0.0",
    "svelte-apollo": "^0.4.0",
    "svelte-fa": "^2.2.0",
    "svelte-routing": "^1.6.0",
    "svelte-spa-router": "^3.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
